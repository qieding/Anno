<ModOps>

  <!-- Fügt Depots die Lagerhausfunktion hinzu -->
  <ModOp Type="replace" Path="//Assets[Asset/Template/GUID='1010519']">
	  <Template>Warehouse</Template>
  </ModOp>	
  
  <ModOp Type="add" GUID='1010519' Path="Values/Building">
	  <BuildingType>Logistic</BuildingType>
  </ModOp>
  
  <ModOp Type="add" GUID='1010519' Path="Values/Selection">
	   <GUIType>Warehouse</GUIType>
  </ModOp>
  
  <ModOp Type="add" GUID='1010519' Path="Values/Warehouse">
       <StorageType>Warehouse</StorageType>
  </ModOp>
  
  <!-- Anzahl Laderampen == 2 -->
  <ModOp Type="replace" GUID='1010519' Path="Values/LogisticNode">  
    <LogisticNode>
      <TransporterCountMax>0</TransporterCountMax>
	  <QueueConfiguration>
	    <ProcessingQueueParallelCount>2</ProcessingQueueParallelCount>
	  </QueueConfiguration> 
	</LogisticNode>
  </ModOp>  
  
  <!-- Kaimauer als Straße -->
  <ModOp Type="replace" GUID='1010567' Path="Values/Street/IsLogicRoad">
	  <IsLogicRoad>1</IsLogicRoad>
  </ModOp>
  
</ModOps>

